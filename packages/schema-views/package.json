  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build:stylable": "stc --srcDir src --outDir esm",
    "build:typescript": "ts-build ./src --cjs --esm",
