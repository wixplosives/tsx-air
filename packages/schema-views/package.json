    "clean": "rimraf ./cjs ./esm",
    "lint": "tslint -p src",
    "prebuild": "yarn clean",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "react": "^16.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "esm",
    "src",
    "!src/tsconfig.json"
  ]
